#启动时要加入这些参数
-Dcatalina.home=D:\sources\tomcat-856-src
-Dcatalina.base=D:\sources\tomcat-856-src
-Djava.endorsed.dirs=D:\sources\tomcat-856-src\endorsed
-Djava.io.tmpdir=D:\sources\tomcat-856-src\temp
-Djava.util.logging.manager=org.apache.juli.ClassLoaderLogManager
-Djava.util.logging.config.file=D:\sources\tomcat-856-src\conf\logging.properties

